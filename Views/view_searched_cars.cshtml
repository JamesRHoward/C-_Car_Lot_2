<h1>Cars fitting your requirements.</h1>
    <ul>
        @foreach(var thing in Model)
            {
                <h3>Make and Model: @thing.GetModel()</h3>
                <li>Price: $@thing.GetMiles()</li>
                <li>Mileage: @thing.GetPrice() Miles</li>
            }
    </ul>
    <h2><a href="/add-new-car">click here to add more cars</a></h2>
    <h2><a href="/search-cars">click here to search your car list</a></h2>
    <h2><a href="/">Go back to home</a></h2>
